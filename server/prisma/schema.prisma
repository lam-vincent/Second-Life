generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int          @id @default(autoincrement())
  username    String       @unique
  email       String       @unique
  password    String
  bulkyWastes BulkyWaste[]
}

model BulkyWaste {
  id          Int      @id @default(autoincrement())
  title       String
  object      String
  streetName  String
  wearRate    Int
  description String
  date        DateTime
  image       String
  userId      Int
  user        User     @relation(fields: [userId], references: [id])
}
